<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="
http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-2.0.xsd">

	<bean id="triangle1" class="com.naironics.four.moreproperties.Triangle">
		<property name="type" value="Equilateral"></property>
	</bean>
	
	<bean id="triangle2" class="com.naironics.four.moreproperties.Triangle">
		<property name="type" value="Isosceles"></property>
	</bean>
	
	<bean id="triangle3" class="com.naironics.four.moreproperties.Triangle">
		<property name="type" value="Scalene"></property>
	</bean>
	

</beans>
